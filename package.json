{
  "name": "frontend-loket",
  "version": "1.0.1",
  "private": true,
  "description": "Frontend of loket",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/frontend-loket.git"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\" --prefixColors auto && npm run format",
    "lint:css": "stylelint --allow-empty-input \"**/*.css\"",
    "lint:format": "prettier . --cache --check",
    "lint:format:fix": "prettier --write --cache \"./**/*.{js,gjs}\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:dependencies": "npx ember dependency-lint",
    "release": "release-it",
    "start": "ember serve",
    "start:new-loket": "NEW_LOKET=true ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test"
  },
  "overrides": {
    "ember-math-helpers": "^3.0.0",
    "ember-source": "$ember-source"
  },
  "overridesComments": {
    "ember-math-helpers": "ember-data-table still brings in v2 but we can't force it to v4 because that's a v2 addon which requires ember-auto-import v2. We can remove the override once this pr is merged: https://github.com/mu-semtech/ember-data-table/pull/35"
  },
  "devDependencies": {
    "@appuniversum/ember-appuniversum": "^3.8.0",
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.1",
    "@embroider/compat": "^3.9.0",
    "@embroider/core": "^3.5.6",
    "@embroider/macros": "^1.17.2",
    "@embroider/router": "^2.1.8",
    "@embroider/webpack": "^4.1.0",
    "@eslint/js": "^9.25.1",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@lblod/ember-acmidm-login": "2.1.0",
    "@lblod/ember-address-search": "^1.0.0",
    "@lblod/ember-mock-login": "^0.10.0",
    "@lblod/ember-submission-form-fields": "^2.24.1",
    "@lblod/submission-form-helpers": "^2.10.0",
    "@sentry/ember": "^7.54.0",
    "broccoli-asset-rev": "^3.0.0",
    "browser-update": "^3.3.38",
    "concurrently": "^9.1.2",
    "ember-async-data": "^2.0.1",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.4.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-deprecation-workflow": "^3.3.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-concurrency": "^3.1.1",
    "ember-data": "~5.4.1",
    "ember-data-table": "^2.1.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.0",
    "ember-moment": "^9.0.0",
    "ember-mu-transform-helpers": "^2.1.3",
    "ember-page-title": "^9.0.1",
    "ember-plausible": "^0.3.0",
    "ember-power-select": "^7.0.0",
    "ember-promise-helpers": "^2.0.0",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.1.0",
    "ember-simple-auth": "^6.0.0",
    "ember-source": "~6.4.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^6.1.0",
    "ember-test-selectors": "^6.0.0",
    "ember-truth-helpers": "^3.0.0",
    "eslint": "^9.25.1",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^15.15.0",
    "loader.js": "^4.7.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.5",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "rdflib": "^2.2.19",
    "release-it": "^15.4.1",
    "sass": "^1.49.7",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^36.0.1",
    "tracked-built-ins": "^3.4.0",
    "tracked-toolbox": "^2.0.0",
    "uuid": "^9.0.0",
    "webpack": "^5.99.7"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
