{
  "name": "frontend-loket",
  "version": "1.1.0",
  "private": true,
  "description": "Frontend of loket",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/frontend-loket.git"
  },
  "license": "MIT",
  "author": "redpencil.io",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:css": "stylelint --allow-empty-input \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:format": "prettier --check --cache \"./**/*.{js,gjs}\"",
    "lint:format:fix": "prettier --write --cache \"./**/*.{js,gjs}\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:dependencies": "npx ember dependency-lint",
    "release": "release-it",
    "start": "vite",
    "start:new-loket": "NEW_LOKET=true ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "vite build --mode test && ember test --path dist"
  },
  "devDependencies": {
    "@appuniversum/ember-appuniversum": "^3.15.0",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^4.0.0",
    "@embroider/compat": "^4.0.3",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.0.3",
    "@embroider/macros": "^1.16.9",
    "@embroider/router": "^3.0.1",
    "@embroider/vite": "^1.1.1",
    "@eslint/js": "^9.12.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@lblod/ember-acmidm-login": "2.1.0",
    "@lblod/ember-address-search": "^1.0.0",
    "@lblod/ember-mock-login": "^0.10.0",
    "@lblod/ember-submission-form-fields": "^3.0.1",
    "@lblod/submission-form-helpers": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@sentry/ember": "^7.54.0",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "browser-update": "^3.3.38",
    "concurrently": "^8.2.2",
    "decorator-transforms": "^2.3.0",
    "ember-async-data": "^2.0.1",
    "ember-auto-import": "^2.8.1",
    "ember-basic-dropdown": "^8.8.0",
    "ember-cli": "~5.12.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-concurrency": "^4.0.0",
    "ember-data": "~5.3.9",
    "ember-load-initializers": "^3.0.0",
    "ember-modifier": "^4.2.0",
    "ember-moment": "^9.0.0",
    "ember-mu-transform-helpers": "^2.1.3",
    "ember-page-title": "^8.2.3",
    "ember-plausible": "^0.3.0",
    "ember-power-select": "^8.0.0",
    "ember-promise-helpers": "^2.0.0",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "^13.0.0",
    "ember-simple-auth": "^6.0.0",
    "ember-source": "~5.12.0",
    "ember-template-lint": "^6.0.0",
    "ember-test-selectors": "^6.0.0",
    "ember-truth-helpers": "^4.0.0",
    "eslint": "^9.12.0",
    "eslint-plugin-ember": "^12.2.1",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^15.11.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "prettier": "^3.3.3",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "qunit": "^2.22.0",
    "qunit-dom": "^3.2.1",
    "rdflib": "^2.2.19",
    "release-it": "^15.4.1",
    "sass": "^1.49.7",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "tracked-built-ins": "^3.3.0",
    "tracked-toolbox": "^2.0.0",
    "uuid": "^9.0.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
