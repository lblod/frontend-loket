<span {{did-insert (perform this.loadData)}}></span>
{{#if this.loadData.lastSuccessful}}
  {{#if this.decisionType}}
    <p>{{this.decisionType.label}}</p>
  {{else}}
    <p class="au-u-muted">Geen informatie beschikbaar</p>
  {{/if}}
  <p class="au-u-muted">{{this.regulationType.label}}</p>
{{/if}}
