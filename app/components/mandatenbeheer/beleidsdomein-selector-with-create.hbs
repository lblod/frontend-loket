<PowerSelectMultipleWithCreate
        @searchEnabled={{true}}
        @disabled={{@disabled}}
        @placeholder={{"Zoek beleidsdomein"}}
        @search={{perform this.searchByName}}
        @selected={{this._beleidsdomeinen}}
        @onChange={{this.select}}
        @onCreate={{this.create}}
        @buildSuggestion={{this.suggest}}
        @showCreatePosition={{"bottom"}}
        @loadingMessage={{"Aan het laden..."}}
        @noMatchesMessage={{"Geen resultaten"}}
        as |beleidsdomein|>
  {{beleidsdomein.label}}
</PowerSelectMultipleWithCreate>
